{
  "name": "React Chrome Extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["main.css"]
    }

  ],
  "action": {
    "default_popup": "./popup.html",
    "default_title": "My Extension"
  },
  "background": {
  },
  "permissions": ["scripting", "activeTab"]
}
