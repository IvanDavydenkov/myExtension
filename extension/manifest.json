{
  "name": "CryptoVolume",
  "version": "1.0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "main.css"
      ]
    }
  ],
  "action": {
    "default_title": "CryptoVolume"
  },
  "background": {
  },
  "permissions": [
    "scripting",
    "activeTab"
  ]
}
